{
   "name": "tts_save",
   "description": "Extract components from a Tabletop Simulator save file.",
   "version": "1.0.0",
   "author": "Eric Brine <ikegami@adaelis.com>",
   "bin": {
      "tts_save": "./bin/run"
   },
   "bugs": "https://github.com/ikegami/tts_save/issues",
   "dependencies": {
      "@oclif/command": "^1.8.0",
      "@oclif/config": "^1.17.0",
      "@oclif/plugin-help": "^3.2.1",
      "got": "^11.8.1",
      "luabundle": "^1.6.0"
   },
   "devDependencies": {
      "@oclif/dev-cli": "^1.26.0",
      "@oclif/test": "^1.2.8",
      "@types/chai": "^4.2.14",
      "@types/mocha": "^8.2.0",
      "@types/node": "^14.14.14",
      "chai": "^4.2.0",
      "eslint": "^7.16.0",
      "eslint-config-oclif": "^3.1.0",
      "eslint-config-oclif-typescript": "^0.2.0",
      "eslint-plugin-unicorn": "^24.0.0",
      "mocha": "^8.2.1",
      "nyc": "^15.1.0",
      "rimraf": "^3.0.2",
      "ts-node": "^9.1.1",
      "tslib": "^2.0.3",
      "typescript": "^4.1.3"
   },
   "_PROBLEM": {
      "eslint-config-oclif": "^3.1.0",
      "eslint-config-oclif-typescript": "^0.2.0"
   },
   "engines": {
      "node": ">=8.0.0"
   },
   "files": [
      "/bin",
      "/lib",
      "/npm-shrinkwrap.json",
      "/oclif.manifest.json"
   ],
   "homepage": "https://github.com/ikegami/tts_save#tts_save_file_tools",
   "keywords": [
      "oclif",
      "tabletop_simulator"
   ],
   "license": "CC0-1.0",
   "main": "lib/index.js",
   "mocha": {
      "recursive": true,
      "reporter": "spec",
      "require": "ts-node/register",
      "spec": "test/**/*.ts",
      "timeout": "2000",
      "watch-extensions": "ts"
   },
   "oclif": {
      "commands": "./lib/commands",
      "bin": "tts_save",
      "plugins": [
         "@oclif/plugin-help"
      ]
   },
   "repository": "ikegami/tts_save",
   "scripts": {
      "postpack": "rimraf oclif.manifest.json",
      "posttest": "eslint . --ext .ts --config .eslintrc",
      "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
      "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
      "version": "oclif-dev readme && git add README.md"
   },
   "types": "lib/index.d.ts"
}
