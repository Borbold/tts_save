{
	"name": "tts_save_extractor",
	"description": "Extract components from a Tabletop Simulator save file.",
	"version": "1.1.0",
	"author": "Eric Brine <ikegami@adaelis.com>",
	"bin": {
		"tts_save_extractor": "./bin/run"
	},
	"bugs": "https://github.com/ikegami/tts_save_extractor/issues",
	"dependencies": {
		"@oclif/command": "^1.8.0",
		"@oclif/config": "^1.17.0",
		"@oclif/plugin-help": "^3.2.0",
		"luabundle": "^1.6.0"
	},
	"devDependencies": {
		"@oclif/test": "^1.2.7",
		"@types/mocha": "^8.0.4",
		"@types/node": "^14.14.9",
		"chai": "^4.2.0",
		"eslint": "^7.14.0",
		"eslint-config-oclif": "^3.1.0",
		"eslint-config-oclif-typescript": "^0.2.0",
		"eslint-plugin-unicorn": "^23.0.0",
		"mocha": "^8.2.1",
		"ts-node": "^9.0.0",
		"tslib": "^2.0.3",
		"typescript": "^4.1.2"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"files": [
		"/bin",
		"/lib"
	],
	"homepage": "https://github.com/ikegami/tts_save_extractor#tts-save-file-extractor",
	"keywords": [],
	"license": "CC0-1.0",
	"main": "lib/index.js",
	"mocha": {
		"recursive": true,
		"reporter": "spec",
		"require": "ts-node/register",
		"spec": "test/**/*.ts",
		"timeout": "5000",
		"watch-extensions": "ts"
	},
	"oclif": {
		"bin": "tts_save_extractor",
		"plugins": [
			"@oclif/plugin-help"
		]
	},
	"repository": "ikegami/tts_save_extractor",
	"scripts": {
		"build": "rm -rf lib && tsc -b",
		"prepack": "rm -rf lib && tsc -b",
		"test": "mocha",
		"posttest": "eslint . --ext .ts --config .eslintrc"
	}
}
